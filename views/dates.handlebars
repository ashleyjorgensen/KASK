<h1>Select Your Date Ideas</h1>

<main>
<div class="container">
<div class="row">
<div class = "col s6'>
{{#each dates}}
{{#unless this.save_date}}
<div>
<form action = "/dates/{{this.id}}?_method = PUT" method = "POST">
<button type = "submit" id = "newDates"> {{this.date_name}} </button>
</form>
</div>
{{/unless}}
{{/each}}
</div>

<h2>Select Some Date Ideas</h2>

<div class="row">
    <div class ="col s12">
<form class="form-group">
    <div class="form-group">
        <label for="dateQuestions">Date Questions:</label>
        <input type="checkbox" id="dateQuestions" name="name">
    </div>

        <button type="submit" class="submiAnswers">Go Spin the Wheel</button>
    </div>
</div>
</form>

