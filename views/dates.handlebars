<h1>Select Your Date Ideas</h1>

<main>
<div class="container">
<div class="row">
<div class = "col s6'>
{{#each dates}}
{{#unless this.save_date}}
<div>
<form action = "/dates/{{this.id}}?_method = PUT" method = "POST">
<button type = "submit" id = "newDates"> {{this.date_name}} </button>
</form>
</div>
{{/unless}}
{{/each}}
</div>