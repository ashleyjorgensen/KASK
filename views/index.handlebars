<h1>Datenight!</h1>

<main>
<div class="container">
<div class="row">
<div class = "col s6'>
{{#each dates}}
{{!-- {{#unless this.save_date = false}} --}}
<div>
<form action = "/dates/{{this.id}}?_method = PUT" method = "POST">
<button type = "submit" id = "newDates"> {{this.date_name}} </button>
</form>
</div>
{{!-- {{/unless}} --}}
{{/each}}
</div>

<li>
    {{name}}
    <button class="declineDate" data-id="{{id}}" data-newDate="{{dates}}">
    </button>
    <button class="rejectDate" data-id="{{this.id}}">Reject Date</button>
</li>